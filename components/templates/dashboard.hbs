<div class="wrap">
    <div class="row margin-tq margin-bb">
        <div class="column all-6">{{>@logo}}</div>
        <div class="column align-right all-6">{{>@button-white}}</div>
    </div>

    <div class="row">
        <div class="column padded-x2">
            <h2 class="p medium">Welcome to your account, Gail</h2>
        </div>
    </div>

    <div class="row collapse margin-bb">
        <div class="column medium-8">
            {{>@card-offset}}
        </div>
        <div class="column medium-4">
            <div class="padding-h padding-tq">
                {{>@list-unformatted}}
            </div>
        </div>
    </div>

    <div class="row">
        <div class="column padded-x2">
            <h2 class="p medium">Your community news and cause</h2>
        </div>
    </div>

    <div class="row margin-bb">
        <div class="column medium-6">
            {{>@card-green-mega}}
        </div>
        <div class="column medium-6">
            {{>@card-with-image-mega}}
        </div>
    </div>

    {{>@header-block}}

    <div class="row margin-bb">
        <div class="column all-11 large-9 large-offset-1">
            <div class="row">
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="column padded-x2">
            <h2 class="p medium">Your Co-op offers</h2>
        </div>
    </div>

    <div class="row margin-bb">
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
    </div>
</div>