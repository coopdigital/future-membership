<div class="wrap">
    <div class="row margin-tq margin-bb">
        <div class="column all-6 padding-q padding-large-0 padding-xlarge-8">{{>@logo}}</div>
        <div class="column align-right all-6 padding-q padding-large-0 padding-xlarge-8">{{>@button-white}}</div>
    </div>

    <div class="row">
        <div class="column padding-q padding-xlarge-24">
            <h2 class="p medium">Welcome to your account, Gail</h2>
        </div>
    </div>

    <div class="row collapse margin-bb">
        <div class="column medium-8">
            {{>@card-offset}}
        </div>
        <div class="column medium-4">
            <div class="padding-h padding-tq">
                {{>@list-unformatted}}
            </div>
        </div>
    </div>

    <div class="row">
        <div class="column padding-q padding-xlarge-24">
            <h2 class="p medium">Your community news and cause</h2>
        </div>
    </div>

    <div class="row collapse margin-bb">
        <div class="column medium-6">
            {{>@card-green-mega}}
        </div>
        <div class="column medium-6">
            {{>@card-with-image-mega}}
        </div>
    </div>

    <div class="row collapse bg-light-turquoise margin-bb">
        <div class="column all-11 large-8 large-offset-1">
            {{>@heading-block}}
        </div>
    </div>

    <div class="row collapse margin-bb">
        <div class="column all-11 large-8 large-offset-1">
            <div class="row">
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
                <div class="column medium-6 margin-bh">
                    {{>@card-underline}}
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="column padding-q padding-xlarge-24">
            <h2 class="p medium">Your Co-op offers</h2>
        </div>
    </div>

    <div class="row separate margin-bb">
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
        <div class="column medium-6 large-4">
            {{>@card-with-image}}
        </div>
    </div>
</div>